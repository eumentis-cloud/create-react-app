{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "react-app",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "no-plusplus": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
